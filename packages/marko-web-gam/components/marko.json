{
  "<marko-web-gam-init>": {
    "template": "./init.marko",
    "@src": "string",
    "@on": "string",
    "@target-tag": "string",
    "@request-frame": "boolean",
    "@init-only": "boolean"
  },
  "<marko-web-gam-head>": {
    "template": "./head.marko",
    "@options": "object",
    "@slots": "object",
    "@targeting": "object"
  },
  "<marko-web-gam-targeting>": {
    "template": "./targeting.marko",
    "@key-values": "object"
  },
  "<marko-web-gam-slots>": {
    "template": "./slots.marko",
    "@slots": "object"
  },
  "<marko-web-gam-enable>": {
    "template": "./enable.marko",
    "<lazy-load>": {
      "@enabled": "boolean",
      "@fetch-margin-percent": "number",
      "@render-margin-percent": "number",
      "@mobile-scaling": "number"
    },
    "@options": "object",
    "@key-values": "object",
    "@include-path": {
      "type": "boolean",
      "default-value": true
    },
    "@include-host": {
      "type": "boolean",
      "default-value": true
    },
    "@include-env": {
      "type": "boolean",
      "default-value": true
    }
  },
  "<marko-web-gam-ad-wrapper>": {
    "template": "./ad-wrapper.marko",
    "@id": "string",
    "@enabled": "boolean",
    "@block-name": "string"
  },
  "<marko-web-gam-define-display-ad>": {
    "template": "./define-display-ad.marko",
    "@id": "string",
    "@path": "string",
    "@inc": "number",
    "@size": "expression",
    "@size-mapping": "array",
    "@targeting": "object",
    "@collapse": "boolean",
    "@collapse-before-ad-fetch": "boolean",
    "@request-on-media-match": "string",
    "@debug-media-query": "boolean",
    "@oop": "boolean",
    "@apply-style": {
      "type": "boolean",
      "default-value": false
    },
    "@class": "string",
    "@modifiers": "array",
    "@attrs": "object",
    "@block-name": {
      "type": "string",
      "default-value": "ad-container"
    },
    "@with-wrapper": "boolean"
  },
  "<marko-web-gam-display-ad>": {
    "template": "./display-ad.marko",
    "@id": "string",
    "@class": "string",
    "@style": "string",
    "@modifiers": "array",
    "@attrs": "object",
    "@block-name": {
      "type": "string",
      "default-value": "ad-container"
    },
    "@slots": "object",
    "@with-wrapper": "boolean"
  },
  "<marko-web-gam-out-of-page-ad>": {
    "template": "./out-of-page-ad.marko",
    "@id": "string",
    "@path": "string",
    "@inc": "number",
    "@class": "string",
    "@style": "string"
  },
  "<marko-web-gam-fixed-ad-bottom>": {
    "template": "./fixed-ad-bottom.marko",
    "@id": "string",
    "@path": "string",
    "@inc": "number",
    "@size": "expression",
    "@size-mapping": "array",
    "@closeable": "boolean",
    "@refresh-interval": "number",
    "@scroll-offset": "number"
  },
  "<marko-web-gam-refresh-ad>": {
    "template": "./refresh-ad.marko",
    "@on": "string",
    "@slot-id": "string",
    "@min-in-view-percentage": "number"
  },
  "<marko-web-gam-inject-ads>": {
    "template": "./inject-ads.marko",
    "@inject <inject>[]": {
      "@at": "number",
      "@id": "string",
      "@path": "string",
      "@inc": "number",
      "@size": "expression",
      "@size-mapping": "array",
      "@targeting": "object",
      "@apply-style": {
        "type": "boolean",
        "default-value": false
      },
      "@class": "string",
      "@modifiers": "array",
      "@block-name": {
        "type": "string",
        "default-value": "ad-container"
      },
      "@collapse-before-ad-fetch": "boolean",
      "@with-wrapper": "boolean"
    },
    "@detect-embeds": "boolean",
    "@selector": "string",
    "@child-selector": "string"
  }
}
