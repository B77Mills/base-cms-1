{
  "taglib-imports": [
    "./blocks/marko.json",
    "./breadcrumbs/marko.json",
    "./content/marko.json",
    "./flows/marko.json",
    "./magazine/marko.json",
    "./page/marko.json",
    "./site-header/marko.json",
    "./site-footer/marko.json",
    "./site-menu/marko.json",
    "./site-navbar/marko.json",
    "./skins/marko.json",
    "./social/marko.json"
  ],
  "<default-theme-menu-toggle-button>": {
    "template": "./menu-toggle-button.marko",
    "@class-name": {
      "type": "string",
      "default-value": "site-navbar__toggler"
    },
    "@targets": {
      "type": "array",
      "default-value": [".site-menu"]
    },
    "@toggle-class": {
      "type": "string",
      "default-value": "site-menu--open"
    },
    "@icon-modifiers": {
      "type": "array",
      "default-value": ["lg"]
    },
    "@icon-name":  {
      "type": "string",
      "default-value": "three-bars"
    },
    "@initially-expanded":  {
      "type": "boolean",
      "default-value": false
    },
    "@button-label": {
      "type": "string",
      "default-value": "Toggle Menu"
    },
    "@expanded-icon-name": "string",
    "@tag": "string"
  }
}
