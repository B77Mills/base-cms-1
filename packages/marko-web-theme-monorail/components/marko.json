{
  "taglib-imports": [
    "./breadcrumbs/marko.json",
    "./blocks/marko.json",
    "./client-side-blocks/marko.json",
    "./flows/marko.json",
    "./gam/marko.json",
    "./identity-x/marko.json",
    "./native-x/marko.json",
    "./nodes/marko.json",
    "./page/marko.json"
  ],
  "<theme-client-side-block-loader>": {
    "template": "./client-side-block-loader.marko",
    "@label": "string",
    "@name": "string",
    "@input": "object"
  },
  "<theme-menu-toggle-button>": {
    "template": "./menu-toggle-button.marko",
    "@class": "string",
    "@before": "string",
    "@before-expanded": "string",
    "@before-collapsed": "string",
    "@after": "string",
    "@after-expanded": "string",
    "@after-collapsed": "string",
    "@targets": "array",
    "@toggle-class": "string",
    "@icon-modifiers": "array",
    "@icon-name": "string",
    "@initially-expanded": "boolean",
    "@expanded-icon-name": "string"
  },
  "<theme-pagination-controls>": {
    "template": "./pagination-controls.marko",
    "@as-rels": "boolean",
    "@per-page": "number",
    "@total-count": "number",
    "@path": "string",
    "@query": "object",
    "@class": "string"

  },
  "<theme-recaptcha>": {
    "template": "./recaptcha.marko"
  },
  "<theme-site-footer>": {
    "template": "./site-footer.marko",
    "@block-name": "string",
    "@tag": "string",
    "@class": "string",
    "@modifiers": "array",
    "@attrs": "object",
    "@has-user": "boolean",
    "@reg-enabled": "boolean"
  },
  "<theme-site-menu>": {
    "template": "./site-menu/index.marko",
    "@block-name": "string",
    "@reg-enabled": "boolean",
    "@has-user": "boolean",
    "@tag": "string",
    "@class": "string",
    "@modifiers": "array",
    "@attrs": "object"
  },
  "<theme-site-newsletter-menu>": {
    "template": "./site-newsletter-menu.marko"
  },
  "<theme-social-follow>": {
    "template": "./social-follow.marko",
    "@block-name": "string",
    "@dark": "boolean",
    "@tag": "string",
    "@class": "string",
    "@modifiers": "array",
    "@attrs": "object"
  },
  "<theme-query-total-count>": {
    "template": "./query-total-count.marko",
    "@params": "object",
    "@name": "string"
  }
}
