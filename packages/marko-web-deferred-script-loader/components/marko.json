{
  "<marko-web-deferred-script-loader-init>": {
    "template": "./init.marko"
  },
  "<marko-web-deferred-script-loader-load>": {
    "template": "./load.marko",
    "@src": "string"
  },
  "<marko-web-deferred-script-loader-call>": {
    "template": "./call.marko",
    "@name": {
      "type": "string",
      "required": true
    },
    "@fn": {
      "type": "string",
      "required": true
    }
  },
  "<marko-web-deferred-script-loader-register>": {
    "template": "./register.marko",
    "@name": {
      "type": "string",
      "required": true
    },
    "@src": {
      "type": "string",
      "required": true
    },
    "@on": "string",
    "@target-tag": "string",
    "@request-frame": "boolean",
    "@init": "string",
    "@init-only": "boolean",
    "@on-script-build": "string",
    "@on-script-load": "string",
    "@attrs": "object"
  }
}
