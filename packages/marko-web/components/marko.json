{
  "taglib-imports": [
    "./document/marko.json",
    "./element/marko.json",
    "./font/marko.json",
    "./load-more/marko.json",
    "./node/marko.json",
    "./node-list/marko.json",
    "./page/marko.json",
    "./resolve/marko.json",
    "./rss/marko.json"
  ],
  "<marko-web-browser-component>": {
    "template": "./browser-component.marko",
    "@name": {
      "type": "string",
      "required": true
    },
    "@hydrate": "boolean",
    "@ssr": "boolean",
    "@skip-when-exists": "boolean",
    "@tag": "string",
    "@class": "string",
    "@attrs": "object",
    "@props": "object"
  },
  "<marko-web-browser-component-script>": {
    "template": "./browser-component/script.marko"
  },
  "<marko-web-deferred-stylesheet>": {
    "template": "./deferred-stylesheet.marko",
    "@href": {
      "type": "string",
      "required": true
    }
  }
}
